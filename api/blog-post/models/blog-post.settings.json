{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog Post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "components": {
      "type": "dynamiczone",
      "components": [
        "shared.seo",
        "sections.email-collection"
      ]
    },
    "email_collection": {
      "type": "component",
      "repeatable": false,
      "component": "sections.email-collection"
    }
  }
}
