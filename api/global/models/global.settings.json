{
  "kind": "singleType",
  "collectionName": "globals",
  "info": {
    "name": "Global",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "favicon": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "siteName": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "defaultSeo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "canonical": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "twitter": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "instagram": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "facebook": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "expanded_footer": {
      "type": "component",
      "repeatable": false,
      "component": "shared.expanded-footer",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "nav_bar": {
      "type": "component",
      "repeatable": false,
      "component": "shared.nav-bar",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "footer_links": {
      "type": "dynamiczone",
      "components": [
        "shared.link-group",
        "shared.text-group"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "nav_links": {
      "type": "dynamiczone",
      "components": [
        "shared.link",
        "shared.link-group",
        "shared.button-link"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "theme": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "json",
      "required": true
    }
  }
}
